{
  "name": "No Doge",
  "description": "Fix twitter logo removes the new X logo from twitter main screen",
  "version": "2.3",
  "manifest_version": 3,
  "content_scripts": [{
    "js": ["no-doge.js"],
    "matches": ["https://twitter.com/*"]
  }],
  "action": {
    "default_icon": "no-doge.png"
  },
  "icons": {
    "128": "no-doge-128.png"
  }
}
